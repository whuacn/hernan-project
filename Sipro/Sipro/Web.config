<?xml version="1.0" encoding="utf-8"?>

<!--
  Para obtener más información sobre cómo configurar la aplicación de ASP.NET, visite
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
  <configSections>
    <section name="Auth" type="Sipro.Providers.AuthenticationProvider, Sipro"/>
  </configSections>
  <Auth name="nametest" test="123456"></Auth>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <authentication mode="Forms">
      <forms loginUrl="logon.aspx" name="AuthCookie" timeout="10" path="/" defaultUrl="/default.aspx" protection="All">
      </forms>
    </authentication>
    <authorization>
      <deny users="?" />
    </authorization>
    <identity impersonate="false" />
  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
    <handlers accessPolicy="Read, Script">
      <remove name="SRT-ASP.NET-ISAPI-4.0-Wildcard" />
      <add name="SRT-ASP.NET-ISAPI-4.0-Wildcard" path="*" verb="*" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll" resourceType="Unspecified" requireAccess="None" preCondition="classicMode,bitness64" responseBufferLimit="0" />
    </handlers>
  </system.webServer>
</configuration>
